<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="deptgrade" extends="st" namespace="/deptgrade">
		<action name="toClassifyManage">
       		<result name="success">/WEB-INF/pages/deptGrade/classifyManage.jsp</result>
        </action>
        
        <action name="toIndexManage">
            <result name="success">/WEB-INF/pages/deptGrade/indexManage.jsp</result>
        </action>
        
        <action name="toGradePercentage">
            <result name="success">/WEB-INF/pages/deptGrade/gradePercentage.jsp</result>
        </action>
        
        <action name="toDeptGrade">
            <result name="success">/WEB-INF/pages/deptGrade/deptGrade.jsp</result>
        </action>
        
        <action name="toDeptGradeDetail">
            <result name="success">/WEB-INF/pages/deptGrade/deptGradeDetail.jsp</result>
        </action>
        
        <action name="toDeptGradeSummarizing">
            <result name="success">/WEB-INF/pages/deptGrade/deptGradeSummarizing.jsp</result>
        </action>
        
        <action name="toDeptFinalScore">
            <result name="success">/WEB-INF/pages/deptGrade/deptFinalScore.jsp</result>
        </action>
        
        <action name="*" class="com.xx.system.deptgrade.action.IndexManageAction" method="{1}"></action>
	</package>
</struts>
